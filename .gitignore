# Ignore node_modules folder (this is generated by npm/yarn)
node_modules/

# Ignore log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore environment variables file (contains sensitive data)
.env

# Ignore local development settings
.DS_Store

# Optional: ignore coverage reports from testing
coverage/

